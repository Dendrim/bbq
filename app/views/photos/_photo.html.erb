<span class="event-photo buffer-both">
  <%= link_to image_tag(photo.photo.thumb.url),
    photo.photo.url, 'data-toggle' => "lightbox",
    'data-gallery' => "event-photos-gallery",
    'data-title' => (
                      'Добавил' + (render partial: 'users/user', object: photo.user) + delete_button(photo)
                    ),
     data: {turbo: false}
  %>
</span>
